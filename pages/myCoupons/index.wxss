.swiper-tab{
    width: 100%;
    text-align: center;
    line-height: 70rpx;
}
.swiper-tab-list{  
    font-size: 28rpx;
    display: inline-block;
    width: 14%;
    color: rgb(0,0,0);
    margin-left: 9%;
    margin-right: 9%;
    text-align: center;
}
.on{ 
    color: rgb(51,51,51);
    border-bottom: 6rpx solid rgb(0,168,101);
}
.swiper-box{ 
  padding: 0 4%;
  display: block; 
  height: 100%; 
  width: 92%; 
  overflow: hidden; 
}
