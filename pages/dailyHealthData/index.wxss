.panel{
  width: 100%; 
  padding: 30rpx;
  box-sizing: border-box;
  border-radius: 2px;
  box-shadow: 0 1px 5px 0 rgba(0, 0, 0, 0.05);
  background-color: #ffffff;
  margin-bottom: 30rpx;
}
.panel_header{
  display: flex;
  justify-content: flex-start;
  align-content: center;
   align-items:center;
   position: relative;
  
}
.title{
  height: 44rpx;  
  font-size: 32rpx;  
  color: #333333;
}
.title_ft::after{
  content: " ";
    display: inline-block;
    height: 24rpx;
    width: 24rpx;
    border-width: 2rpx 2rpx 0 0;
    border-color: #333;
    border-style: solid;
    -webkit-transform: matrix(.71,.71,-.71,.71,0,0);
    transform: matrix(.71,.71,-.71,.71,0,0);
    position: absolute;
    top: 50%;
    margin-top: -10rpx;
    right: 0;
   
}
